<!DOCTYPE rdf:RDF [
    <!-- Other namespaces -->
    <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dct  "http://dublincore.org/documents/dcmi-terms/" >
    <!ENTITY ans  "http://www.w3.org/2000/10/annotation-ns#" >
    <!ENTITY bkm  "http://www.w3.org/2002/01/bookmark#" >
    <!ENTITY pav  "http://purl.org/pav/">
    <!ENTITY rdfg "http://www.w3.org/2004/03/trix/rdfg-1/">
    
    <!-- Main namespace -->
    <!ENTITY annotation "http://purl.org/ao/" >
]>

<rdf:RDF
    xml:base="&annotation;"
    xmlns="&annotation;"
    xmlns:rdfg="&rdfg;"
    xmlns:pav="&pav;"
    xmlns:rdfs="&rdfs;"
    xmlns:owl="&owl;"
    xmlns:rdf="&rdf;"
    xmlns:xsd="&xsd;"
    xmlns:dct="&dct;"
    xmlns:ans="&ans;"
    xmlns:bkm="&bkm;">
    
    <owl:Ontology rdf:about="&annotation;">
        <rdf:type rdf:resource="&owl;Ontology"/>
        <rdfs:label rdf:datatype="&xsd;string">Annotation Ontology v. 2.0b</rdfs:label>
        
        <owl:imports rdf:resource="http://purl.org/pav/2.0/"/>

    	<dct:language>en</dct:language>
    	<dct:title xml:lang="en">Annotation Ontology</dct:title>
    	<dct:creator rdf:resource="http://www.paolociccarese.info"/>
    	<dct:publisher rdf:resource="http://www.mindinformatics.org"/>
    	<dct:contributor rdf:datatype="&xsd;string">Paolo Ciccarese</dct:contributor>
        <dct:created rdf:datatype="&xsd;string">February 5, 2012</dct:created>
        <dct:date rdf:datatype="&xsd;string">February 5, 2012</dct:date>
        <dct:format rdf:datatype="&xsd;string">rdf/xml</dct:format>    
    </owl:Ontology> 
  
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
     
    <rdf:Description rdf:about="&dct;title">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;created">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;date">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;creator">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;format">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;language">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;publisher">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;contributor">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>          
    
      <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Ontology Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
  	
    <owl:Class rdf:about="Annotation" rdfs:label="(ao) Annotation">
    	<rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <owl:disjointWith rdf:resource="Selector"/>
        <owl:disjointWith rdf:resource="AnnotationSet"/>
    	
        <rdfs:comment rdf:datatype="&xsd;string">
            Generic annotation class.
        </rdfs:comment>
        
        <!-- created by -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdBy"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
		
        <!-- created on -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdOn"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- annotates -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="annotatesResource"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- selector -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="context"/>
                <owl:someValuesFrom>
                    <owl:Class rdf:about="Selector"/>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="context"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        
        <!-- has topic -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="hasTopic"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- body -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="body"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <owl:Class rdf:about="Selector" rdfs:label="(ao) Selector">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <owl:disjointWith rdf:resource="Annotation"/>
        <owl:disjointWith rdf:resource="AnnotationSet"/>
    	
        <rdfs:comment rdf:datatype="&xsd;string">
            Selectors identify document fragments.
        </rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="AnnotationSet" rdfs:label="(ao) Annotation Set">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <owl:disjointWith rdf:resource="Annotation"/>
        <owl:disjointWith rdf:resource="Selector"/>   
    
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
  		
        <!-- created by -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdBy"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
		
        <!-- created on -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdOn"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- has topic -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="hasTopic"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- items -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="item"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="Annotation"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="item"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- source document -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="onResource"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Ontology Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:ObjectProperty rdf:about="onResource">
    </owl:ObjectProperty> 
    
    <owl:ObjectProperty rdf:about="annotatesResource">
    </owl:ObjectProperty> 
    
    <owl:ObjectProperty rdf:about="onSourceDocument">
    	<rdfs:range rdf:resource="&pav;SourceDocument" />
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="hasTopic">
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="body">
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="body">
    </owl:DatatypeProperty>
    
    <owl:ObjectProperty rdf:about="item">
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&pav;curates">
        <owl:inverseOf rdf:resource="&pav;curatedBy" />
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="status">
    </owl:DatatypeProperty>
    
    <owl:ObjectProperty rdf:about="context">
    	<rdfs:domain rdf:resource="Annotation" />
    	<rdfs:range rdf:resource="Selector" />
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // XPointer Selectors
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="XPointerSelector" rdfs:label="(ao-sel) XPointer Selector">
    	<rdfs:subClassOf rdf:resource="Selector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            The XPointer Selector allows to identify any part of an XML document by using the XPointer technology 
        </rdfs:comment>
        <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="xpointer"/>
	            <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>	
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="xpointer">
    </owl:DatatypeProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Text Selectors
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="TextSelector" rdfs:label="(ao-sel) Text Selector">
    	<rdfs:subClassOf rdf:resource="Selector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            A Text Selector identifies a string in a document. 
        </rdfs:comment>
        
        <!-- exact match -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="exact"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="exact"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="exact">
     	<rdfs:comment rdf:datatype="&xsd;string">
            The exact string - a linear sequence of characters - subject of the annotation.
        </rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:Class rdf:about="OffsetRangeTextSelector" rdfs:label="(ao-sel) Offset/Range Text Selector">
    	<rdfs:subClassOf rdf:resource="TextSelector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            A Offset Range Text Selector identifies a string in a document through an offset 
            -  an integer indicating the distance (displacement) from the beginning of the document up until a 
            given element or point, within the same document - and a range - an integer indicating the number 
            of characters, starting from the offset, identified by the selector.
        </rdfs:comment>
        
        <!-- offset and range -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="offset"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="offset"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="range"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="range"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="offset">
    	<rdfs:comment rdf:datatype="&xsd;string">
            An integer indicating the distance (displacement) from the beginning of the document up until a 
            given element or point, within the same document.
        </rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="range">
      	<rdfs:comment rdf:datatype="&xsd;string">
            An integer indicating the number of characters - starting from the offset - identified by the selector.
        </rdfs:comment>  	
    </owl:DatatypeProperty>
    
    <owl:Class rdf:about="PrefixPostfixTextSelector" rdfs:label="(ao-sel) Prefix/Postfix Text Selector">
    	<rdfs:subClassOf rdf:resource="TextSelector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            A Prefix Postfix Text Selector identifies a string in a document through the prefix - a number of contiguos characters 
            before the exact match - and a postfix - a number of contiguos characters after the exact match.
        </rdfs:comment>
        
        <!-- offset and range -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="prefix"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="prefix"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="postfix"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
         <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="postfix"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="prefix">
    	<rdfs:comment rdf:datatype="&xsd;string">
            A number of contiguos characters - string - before the exact match.
        </rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="postfix">
    	<rdfs:comment rdf:datatype="&xsd;string">
            A number of contiguos characters - string - after the exact match.
        </rdfs:comment>
    </owl:DatatypeProperty>    
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Image Selectors
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="ImageSelector" rdfs:label="(ao-sel) Image Selector">
    	<rdfs:subClassOf rdf:resource="Selector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            An Image Selector identifies a portion of an image.
        </rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="InitEndCornerSelector" rdfs:label="(ao-sel) Image Init End Corner Selector">
    	<rdfs:subClassOf rdf:resource="ImageSelector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            An Image Selector where the portion of an image is identified by a rectangle that is specified
            through the left upper corner and the bottom right corner.
        </rdfs:comment>
        
        <!-- init and end -->
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="init"/>
	            <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>	
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="init"/>
	            <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="end"/>
	            <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="end"/>
	            <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>
    </owl:Class>
    
    <owl:DatatypeProperty rdf:about="init">
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="end">
    </owl:DatatypeProperty> 
        
        
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Audio Selectors
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->        
    <owl:Class rdf:about="AudioSelector" rdfs:label="(ao-sel) Audio Selector">
    	<rdfs:subClassOf rdf:resource="Selector"/>
        <rdfs:comment rdf:datatype="&xsd;string">
            An Audio Selector identifies a portion of an audio file.
        </rdfs:comment>
        
        <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="init"/>
	            <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>	
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="init"/>
	            <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="end"/>
	            <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>
	    <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="end"/>
	            <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
	        </owl:Restriction>
	    </rdfs:subClassOf>
    </owl:Class>
 
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Type Qualifiers
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="Qualifier" rdfs:label="(ao-type) Qualifier">
    	<rdfs:subClassOf rdf:resource="Annotation"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
    </owl:Class> 
    
    <owl:Class rdf:about="ExactQualifier" rdfs:label="(ao-type) Exact Qualifier">
    	<rdfs:subClassOf rdf:resource="Qualifier"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
    </owl:Class> 
   
    <owl:Class rdf:about="CloseQualifier" rdfs:label="(ao-type) Close Qualifier">
    	<rdfs:subClassOf rdf:resource="Qualifier"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
    </owl:Class> 
    
    <owl:Class rdf:about="NarrowQualifier" rdfs:label="(ao-type) Narrow Qualifier">
    	<rdfs:subClassOf rdf:resource="Qualifier"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
    </owl:Class> 
    
    <owl:Class rdf:about="BroadQualifier" rdfs:label="(ao-type) Broad Qualifier">
    	<rdfs:subClassOf rdf:resource="Qualifier"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
    </owl:Class> 

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Type Graph
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->    
    <owl:Class rdf:about="GraphAnnotation" rdfs:label="(ao-type) Graph Annotation">
    	<rdfs:subClassOf rdf:resource="Annotation"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
        
        <rdfs:subClassOf>
	        <owl:Restriction>
	            <owl:onProperty rdf:resource="body"/>
	            <owl:allValuesFrom rdf:resource="&rdfg;Graph"/>
	        </owl:Restriction>
	    </rdfs:subClassOf>
    </owl:Class>
    
    <owl:Class rdf:about="&rdfg;Graph" rdfs:label="Graph"/>
     
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Type Comment
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="Comment" rdfs:label="(ao-type) Comment">
    	<rdfs:subClassOf rdf:resource="Annotation"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
        
        <!-- body -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="body"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Type Note
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="Note" rdfs:label="(ao-type) Note">
