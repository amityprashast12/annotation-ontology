        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
        
        <!-- body -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="body"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Type Definition
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="Definition" rdfs:label="(ao-type) Definition">
    	<rdfs:subClassOf rdf:resource="Annotation"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>	
        
         <!-- body -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="body"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Type Example
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:Class rdf:about="Example" rdfs:label="(ao-type) Example">
    	<rdfs:subClassOf rdf:resource="Annotation"/>
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
        
         <!-- body -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="body"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
</rdf:RDF>